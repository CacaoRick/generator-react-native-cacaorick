{
	"compilerOptions": {
		"target": "es6",
		"module": "es6",
		"moduleResolution": "node",
		"jsx": "react-native",
		"outDir": "build",
		"rootDir": "src",
		"allowJs": false,
		"sourceMap": true
	},
	"types": [
		"react",
		"react-native",
		"jest"
	],
	"exclude": [
		"App.js",
		"index.android.js",
		"index.ios.js",
		"android",
		"ios",
		"build",
		"node_modules"
	],
	"compileOnSave": false
}
